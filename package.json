{
	"name": "my-social-media-backend",
	"version": "1.0.0",
	"description": "RESTful api for social media",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"dev": "nodemon index.js",
		"migrate:up": "migrate-mongo up",
		"migrate:down": "migrate-mongo down",
		"test": "cross-env NODE_ENV=test jest --coverage",
		"test:detectOpenHandles": "cross-env NODE_ENV=test jest --detectOpenHandles",
		"prettify": "prettier --write 'src/**/*.js' '__tests__/**/*.js' 'migrations/**/*.js'",
		"check": "eslint src/**/*"
	},
	"keywords": [],
	"author": "micael-jerry",
	"license": "ISC",
	"repository": {
		"url": "https://github.com/micael-jerry/my-social-media-backend"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"eslint": "^8.54.0",
		"jest": "^29.7.0",
		"migrate-mongo": "^11.0.0",
		"nodemon": "^3.0.1",
		"prettier": "^3.1.0",
		"supertest": "^7.0.0"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"date-fns": "^3.6.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"http-status-codes": "^2.3.0",
		"joi": "^17.13.0",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^8.0.1",
		"validator": "^13.11.0"
	},
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		],
		"testPathIgnorePatterns": [
			"/__tests__/conf/"
		]
	}
}
